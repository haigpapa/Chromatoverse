{
  "name": "codeverse-explorer",
  "version": "1.0.0",
  "description": "AI-powered 3D code visualization platform",
  "scripts": {
    "analyze": "node analyze.js .",
    "analyze:copy": "npm run analyze && cp codeverse-data.json client/public/",
    "client": "cd client && npm run dev",
    "client:build": "cd client && npm run build",
    "client:install": "cd client && npm install",
    "start": "npm run analyze:copy && npm run client"
  },
  "keywords": [
    "code-visualization",
    "3d",
    "developer-tools",
    "ai",
    "code-analysis"
  ],
  "author": "Haig (Walaw Studio)",
  "license": "MIT"
}
